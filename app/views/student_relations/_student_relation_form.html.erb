<% form_for(relative, :html => {:class => "default"}) do |f| %>
  <%= f.error_messages %>
	
	<div class="clearfix">
		<div class="p1">
		<% f.fields_for :person do |person_fields| %>
			<%= render :partial => "people/person_fields", :locals => {:f => person_fields, :person => relative.person} %>
		<% end %>
		</div><!-- end .p1-->
			
		<div class="p2">
			<% field_set_tag "Related Students" do %>
				
			
			<% end %>
		</div><!-- end .p2 -->
	</div>
  <p>
    <%= f.submit %>
  </p>
<% end %>
