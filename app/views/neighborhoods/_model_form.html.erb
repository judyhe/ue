
<% form_for(neighborhood, :html => {:class => "default"}) do |f| %>
  <%= f.error_messages %>
	
	<div class="input_set clearfix">
		<span>
			<%= f.label :ord, "Order" %>
			<%= f.text_field :ord, :size => 1 %>
		</span>
		<span>
			<%= f.label :name %>
			<%= f.text_field :name %>
		</span>
	</div>
	
	<div>
		<%= f.label :county_id %>
		<%= f.collection_select :county_id, County.all, :id, :name, {:include_blank => true} %>
	</div>
  <p>
    <%= f.submit %>
  </p>
<% end %>

